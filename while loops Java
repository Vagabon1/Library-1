import java.util.Scanner;

public class Main {
   public static void main(String[] args) {
       Scanner read = new Scanner(System.in);
       int number = read.nextInt();
        int z = 0;

       //your code goes here
      while (number > z) {
        z ++;
        if (!(z % 3 == 0) && (!(z % 10 == 3))) {
            continue;
            }
        System.out.println(z);
        }
    }
}
